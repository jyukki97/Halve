#include <iostream>using namespace std;char a[8][8];int main(void){    int cnt = 0;    for(int i=0;i<8;i++){        for(int j=0;j<8;j++)            cin >> a[i][j];    }    for(int i=0;i<8;i++){        for(int j=0;j<8;j++){            if(((j%2 == 0 && i%2 == 0) || (j%2 == 1 && i%2 == 1)) && a[i][j] == 'F')                cnt ++;        }    }    cout << cnt << endl;    return 0;}
